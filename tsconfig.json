{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "skipLibCheck": true
  },
  "lib": [
    "es6"
  ],
  "exclude": [
    "node_modules",
    "./*.ts",
    "__test__"
  ]
}